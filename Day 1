x=10
s=""
while x!=0:
    s=str(x%2)+s
    x=x//2
print(s)
#print(bin(10))


import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String sb="";
		int n=sc.nextInt();
		while (n!=0){
		    sb=n%2+sb;
		    n=n/2;
		}
		System.out.print(sb);
	}
}
